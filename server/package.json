{
  "name": "tfwmma-lifting-server",
  "version": "1.0.0",
  "description": "Backend for TFW MMA Lifting Program",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testMatch='**/?(*.)+(integration).[jt]s?(x)'",
    "test:unit": "jest --testMatch='**/?(*.)+(unit).[jt]s?(x)'",
    "seed": "ts-node src/utils/seedDatabase.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.5.2",
    "@sentry/integrations": "^7.114.0",
    "@sentry/node": "^9.14.0",
    "@sentry/profiling-node": "^9.14.0",
    "@sentry/types": "^9.14.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.10",
    "@types/supertest": "^2.0.12",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "msw": "^1.3.2",
    "prisma": "^5.5.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
